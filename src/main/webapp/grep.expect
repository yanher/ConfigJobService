set timeout 300000
set ip [lindex $argv 0]
set workspace [lindex $argv 1]
set jobName [lindex $argv 2]
set command [lindex $argv 3]
spawn ssh root@$ip
expect "#"
send "cd $workspace \r"
expect "#"
send "cd logs \r"
expect "#"
set jobLog ${jobName}.log
send "echo start_execute \r"
expect "#"
send "cat $jobLog|$command \r"
expect "#"
send "exit \r"
expect eof
